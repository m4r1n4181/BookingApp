<Window x:Class="BookingApp.View.OwnerWindows.OwnerMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Owner Main Window"
        Height="450"
        Width="800">
   
    <StackPanel>
        <Label Content="Super Owner"
               Visibility="{Binding SuperOwnerVisibility}" />
        <Button Content="My Accommodations"
                Command="{Binding MyAccommodationsCommand}" />
        <Button Content="Log Out"
                Command="{Binding LogOutCommand}" />
        <Button Content="Schedule Renovation"
                Command="{Binding ScheduleRenovationCommand}" />
        <Button Content="Scheduled Renovations"
                Command="{Binding ScheduledRenovationsCommand}" />
        <Label Content="DODALA SAM SORT ZA SMESTAJ AKO JE OD SUPER VLASNIKA PROVERITE TO"
               FontSize="20"
               FontWeight="Bold"/>
    </StackPanel>
</Window>
